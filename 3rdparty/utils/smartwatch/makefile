	@$(ECHO) "*                                                *"
	@$(ECHO) "*               SmartWatch Utilities             *"
	@$(ECHO) "*                                                *"
	@$(ECHO) "**************************************************"
 
clean:
	$(RM) $(ALLOBJS)

