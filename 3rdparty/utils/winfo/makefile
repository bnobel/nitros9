include ../../../Makefile.rules

DEPENDS		= ./Makefile

CMDS		= witesta
SUBS		= winfo

ALLOBJS		= $(CMDS) $(SUBS)

all:	$(ALLOBJS) $(DEPENDS)	
	@$(ECHO) "*********************************************"
	@$(ECHO) "*                                           *"
	@$(ECHO) "*                WInfo Utility              *"
	@$(ECHO) "*                                           *"
	@$(ECHO) "*********************************************"
 
clean:
	$(RM) $(ALLOBJS)

