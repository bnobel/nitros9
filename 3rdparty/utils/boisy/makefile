# OS-9 Commands/Utilities
#

include ../../../Makefile.rules

DEPENDS		= ./Makefile

CMDS		= bawk del grep mode send timer bigbro font makdir \
		more tee

ALLOBJS		= $(CMDS)

all:	$(ALLOBJS) $(DEPENDS)	
	@$(ECHO) "*********************************************"
	@$(ECHO) "*                                           *"
	@$(ECHO) "*          Boisy Pitre's Utilities          *"
	@$(ECHO) "*                                           *"
	@$(ECHO) "*********************************************"
 
clean:
	$(RM) $(ALLOBJS)

