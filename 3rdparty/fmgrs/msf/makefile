# MSF file manager
#

include ../../../Makefile.rules

DEPENDS		= ./Makefile

FMGRS		= msf_6309.mn msf_6809.mn
DESCS		= m0_40d.dd
ALLOBJS		= $(FMGRS) $(DESCS)

all:	$(ALLOBJS) $(DEPENDS)

clean:
	$(RM) $(ALLOBJS)

msf_6309.mn: msf.asm
	$(AS) $(AFLAGS) $< $(ASOUT)$@ -e -aH6309=1

msf_6809.mn: msf.asm
	$(AS) $(AFLAGS) $< $(ASOUT)$@

