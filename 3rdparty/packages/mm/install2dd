***************************************************
* Microscopic Mission Install Script for the CoCo 3
* Files are copied to the /DD device
*
* Creating directories
makdir /DD/GAMES
makdir /DD/GAMES/MM
makdir /DD/GAMES/MM/CMDS
* Copying commands to /DD/GAMES/MM/CMDS
copy -r CMDS/AutoEx /DD/GAMES/MM/CMDS/mm
copy -r CMDS/mod1.dat -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod2.dat -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod3.dat -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod4.dat -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod5.dat -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod6.sbr -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod7.sbr -w=/DD/GAMES/MM/CMDS
copy -r CMDS/mod8.sbr -w=/DD/GAMES/MM/CMDS
* Copying support files to /DD/GAMES/MM
copy -r mm.ttl -w=/DD/GAMES/MM
copy -r mm.lib -w=/DD/GAMES/MM
copy -r mm.pnl -w=/DD/GAMES/MM
copy -r global.dat -w=/DD/GAMES/MM
copy -r mod7.lib -w=/DD/GAMES/MM
* All Done!
