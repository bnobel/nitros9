**********************************************
* Kyum-Gai Install Script for the CoCo 3
* Files are copied to the /DD device
*
* Creating directories
makdir /DD/GAMES
makdir /DD/GAMES/KYUMGAI
makdir /DD/GAMES/KYUMGAI/CMDS
* Copying commands to /DD/GAMES/KYUMGAI/CMDS
copy -r CMDS/AutoEx /DD/GAMES/KYUMGAI/CMDS/ninja
copy -r CMDS/ninja.snd1 -w=/DD/GAMES/KYUMGAI/CMDS
copy -r CMDS/ninja.snd2 -w=/DD/GAMES/KYUMGAI/CMDS
* Copying support files to /DD/GAMES/KYUMGAI
copy -r backgnd.dat -w=/DD/GAMES/KYUMGAI
copy -r backgnd2.dat -w=/DD/GAMES/KYUMGAI
copy -r bigblocks.dat -w=/DD/GAMES/KYUMGAI
copy -r blakguy.dat -w=/DD/GAMES/KYUMGAI
copy -r gameover.dat -w=/DD/GAMES/KYUMGAI
copy -r giant.dat -w=/DD/GAMES/KYUMGAI
copy -r kyumgai.pla -w=/DD/GAMES/KYUMGAI
copy -r level1.dat -w=/DD/GAMES/KYUMGAI
copy -r level2.dat -w=/DD/GAMES/KYUMGAI
copy -r level3.dat -w=/DD/GAMES/KYUMGAI
copy -r level4.dat -w=/DD/GAMES/KYUMGAI
copy -r level5.dat -w=/DD/GAMES/KYUMGAI
copy -r level6.dat -w=/DD/GAMES/KYUMGAI
copy -r level7.dat -w=/DD/GAMES/KYUMGAI
copy -r level8.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap1.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap2.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap3.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap4.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap5.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap6.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap7.dat -w=/DD/GAMES/KYUMGAI
copy -r monmap8.dat -w=/DD/GAMES/KYUMGAI
copy -r ninja.dat -w=/DD/GAMES/KYUMGAI
copy -r objects.dat -w=/DD/GAMES/KYUMGAI
copy -r playscn.vef -w=/DD/GAMES/KYUMGAI
copy -r staffguy.dat -w=/DD/GAMES/KYUMGAI
copy -r tanguy.dat -w=/DD/GAMES/KYUMGAI
copy -r tobe.pla -w=/DD/GAMES/KYUMGAI
copy -r tobe.vef -w=/DD/GAMES/KYUMGAI
copy -r window.dat -w=/DD/GAMES/KYUMGAI
copy -r winner.vef -w=/DD/GAMES/KYUMGAI
* All Done!
